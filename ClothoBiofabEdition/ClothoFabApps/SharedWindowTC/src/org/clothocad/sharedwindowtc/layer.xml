<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-clothocad-sharedwindowtc-SharedAction.instance">
                <attr name="component" methodvalue="org.clothocad.sharedwindowtc.SharedTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.clothocad.sharedwindowtc.Bundle#CTL_SharedAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="SharedAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-clothocad-sharedwindowtc-SharedAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="SharedTopComponent.settings" url="SharedTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="properties">
                <file name="SharedTopComponent.wstcref" url="SharedTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
