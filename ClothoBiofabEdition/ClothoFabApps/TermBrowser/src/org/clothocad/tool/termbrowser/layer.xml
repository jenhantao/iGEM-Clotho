<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-clothocad-tool-termbrowser-BrowserAction.instance">
                <attr name="component" methodvalue="org.clothocad.tool.termbrowser.BrowserTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.clothocad.tool.termbrowser.Bundle#CTL_BrowserAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="BrowserAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-clothocad-tool-termbrowser-BrowserAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="BrowserTopComponent.settings" url="BrowserTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="leftSlidingSide">
                <file name="BrowserTopComponent.wstcref" url="BrowserTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
    <folder name="plugins">
        <folder name="tools">
            <file name="org-clothocad-tool-termbrowser-Connect.instance">
                <attr name="instanceOf" stringvalue="org.clothocore.api.plugin.ClothoTool"/>
            </file>
        </folder>
        <folder name="xml">
            <file name="termbrowser.xml" url="termbrowser.xml"></file>
        </folder>
    </folder>
</filesystem>
